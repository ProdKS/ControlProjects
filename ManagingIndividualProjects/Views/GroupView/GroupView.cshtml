@model List<Group>
@{
    ViewData["Title"] = "Админ страница(Группы)";
}
<body>
    <div class="title-div">
        <h1 class="title">Группы</h1>
    </div>
    <div class="container-deprtaments">
        @foreach (var groups in Model)
        {
            <div class="card bg-light d-flex flex-fill" style="margin-top: 20px;">
                <a class="card-header text-muted border-bottom-0" style="text-align: center;" href="@Url.Action("GroupDetailView", "GroupView", new {groupsId = groups.Id})">@groups.Name</a>
            </div>
        }
    </div>
</body>